    //$output1 = 无
    //$directions = 如果

    #define   If(__text)             if(__text)
    #define   Then                  {
    #define   Endif                 }

     #if(BLOCK_CODE)
If(    )
Then

  Endif
    #endif

    #if(WORK_SHOP_SAMPLE)
