#if(WORK_SHOP_COMMAND)
  //$int
  #if(___EN)
    //$name = Interrupt_Funtion
    //$input1 = Address,Title
    //$output1 = no
    //$directions = Interrupt the function instruction, fill in the interrupt Address and Title, write the interrupt execution program between Interrupt_Funtion(Address,Title) and Function_End

    #define Interrupt_Funtion(Address,Title)         void __attribute((interrupt(Address))) Title(void){         
    #define   Function_End                            }
    
    #if(BLOCK_CODE)
Interrupt_Funtion(4,Interrupt_Extemal)

  Function_End
    #endif

    #if(WORK_SHOP_SAMPLE)
      Interrupt_Funtion(4,Interrupt_Extemal)
      Function_End
    #endif

  #endif

  #if(___CH)
    //$name = 中斷函數
    //$input1 = 中斷地址,中斷函數名稱
    //$output1 = 無
    //$directions = 中斷函數指令，填寫中斷地址以及名稱后，在Interrupt_Funtion(Address,Title)和Function_End之間寫入中斷執行的程序

    #define Interrupt_Funtion(Address,Title)         void __attribute((interrupt(Address))) Title(void){         
    #define   Function_End                            }
    
    #if(BLOCK_CODE)
Interrupt_Funtion(4,Interrupt_Extemal)

  Function_End
    #endif

    #if(WORK_SHOP_SAMPLE)
      Interrupt_Funtion(4,Interrupt_Extemal)
      Function_End
    #endif

  #endif

    #if(___CH_S)
    //$name = 中断函数
    //$input1 = 中断地址,中断函数名称
    //$output1 = 无
    //$directions = 中断函数指令，填写中断地址以及名称后，在Interrupt_Funtion(Address,Title)和Function_End之间写入中断执行的程序

    #define Interrupt_Funtion(Address,Title)         void __attribute((interrupt(Address))) Title(void){         
    #define   Function_End                            }
    
    #if(BLOCK_CODE)
Interrupt_Funtion(4,Interrupt_Extemal)

  Function_End
    #endif

    #if(WORK_SHOP_SAMPLE)
      Interrupt_Funtion(4,Interrupt_Extemal)
