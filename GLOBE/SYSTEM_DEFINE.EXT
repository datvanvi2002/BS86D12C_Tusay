#if(WORK_SHOP_COMMAND)
  //$system
  #if(___EN)
    //$name = For loop
    //$input1 = The content of the loop is executed n times
    //$output1 = no
    //$directions= For the following... to Next content loop execution n times
    
    #define   For(n)               int i;\
                                    for(i=0;i<n;i++){
    #define   Next                 }
    
     #if(BLOCK_CODE)
      For(Loop N times)
        //Execute n times


      Next  //If the loop is not over, execute For, and continue to go down when it ends
    #endif

    #if(WORK_SHOP_SAMPLE)
For(Loop N times)
//Execute n times,The variable i will increase from 0 to n
      
  Next  //If the loop is not over, execute For, and continue to go down when it ends
    #endif
    
  #endif
  
  #if(___CH)
    //$name = For循環
    //$input1 = 循環內容執行n次
    //$output1 = 無
    //$directions= 以下至Next的內容循環執行 n 次

        #define   For(n)               int i;\
                                    for(i=0;i<n;i++){
    #define   Next                 }
    

    #if(BLOCK_CODE)
For(   )
//執行n次內容,變量i將從0依次增加到n


  Next  //若循環未結束，則執行For，結束則繼續往下走
    #endif



    #if(WORK_SHOP_SAMPLE)
      For(循環n次)
        //執行n次內容
      Next  //若循環未結束，則執行For，結束則繼續往下走
    #endif
  #endif

      #if(___CH_S)
      //$name = For循环
      //$input1 = 循环内容执行n次
      //$output1 = 无
      //$directions= 以下至Next的内容循环执行 n 次

      #define   For(n)               int i;\
                                    for(i=0;i&lt;n;i++){
      #define   Next                 }

      #if(BLOCK_CODE)
For(   )    
//执行n次内容,变量i将从0依次增加到n


  Next  //若循环未结束，则执行For，结束则继续往下走
      #endif


      #if(WORK_SHOP_SAMPLE)
      For(循环n次)

      //执行n次内容
      Next  //若循环未结束，则执行For，结束则继续往下走
      #endif
      #endif

#endif


#if(WORK_SHOP_COMMAND)
  //$system
  #if(___EN)
    //$name = While condition
    //$input1 = condition
    //$output1 = no
    //$directions = the following... to Wend content loop execution When the condition is satisfied.Stop the loop until the condition isn't satisfied and keep going
    
    #define   While(__text)          while(__text){
      
    #define   Wend                    }

    #if(BLOCK_CODE)
While(   )
//If the condition is satisfied, execute the content, otherwise  continue to go down


  Wend  //Return While to continue the loop
    #endif

    #if(WORK_SHOP_SAMPLE)
      While(Judgment or Boolean)
        //If the condition is satisfied, execute the content, otherwise  continue to go down
      Wend  //Return While to continue the loop
    #endif

  #endif
    
  #if(___CH)
    //$name =While 條件
    //$input1 =條件
    //$output1 = 無
    //$directions = 當條件滿足時循環執行以下至Wend的內容，直到條件不滿足時結束循環後繼續往下走

    #define   While(__text)          while(__text){
      
    #define   Wend                    }

    #if(BLOCK_CODE)
While(   )
//條件滿足執行內容，否則繼續往下走


  Wend  //返回While繼續循環
#endif

    #if(WORK_SHOP_SAMPLE)
      While(判斷式或者布林)
        //條件滿足執行內容，否則繼續往下走
      Wend  //返回While繼續循環
    #endif
  #endif

    #if(___CH_S)
    //$name =While 条件
    //$input1 =条件
    //$output1 = 无
    //$directions = 当条件满足时循环执行以下至Wend的内容，直到条件不满足时结束循环后继续往下走

    #define   While(__text)          while(__text){
    #define   Wend                    }

    #if(BLOCK_CODE)
While(   )
//条件满足执行内容，否则继续往下走



Wend  //返回While继续循环
#endif

    #if(WORK_SHOP_SAMPLE)
    While(判断式或者布林)

    //条件满足执行内容，否则继续往下走
    Wend  //返回While继续循环
    #endif
    #endif

#endif



